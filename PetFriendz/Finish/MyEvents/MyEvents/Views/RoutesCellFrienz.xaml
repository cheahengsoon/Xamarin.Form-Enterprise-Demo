<?xml version="1.0" encoding="utf-8" ?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyEvents.Views.RoutesCellFrienz">
  <StackLayout Orientation="Horizontal"  Spacing="10" BackgroundColor="White" Padding="10">
    <Image x:Name="imageRoute" Source="person.jpg" Aspect="AspectFill" ></Image>
    <StackLayout Orientation="Vertical" VerticalOptions="Center">
      <Label x:Name="fromLabel" TextColor="Blue" VerticalOptions="Center" FontSize="20"></Label>
      <Label Text="Followed You"/>
      <Label x:Name="toLabel" TextColor="Green" VerticalOptions="Center" FontSize="20"></Label>

    </StackLayout>
    <!--<Button Text="Cancel" ></Button>-->
  </StackLayout>
</ViewCell>
